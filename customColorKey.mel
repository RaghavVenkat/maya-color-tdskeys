// Set TDS Key color using picker
setKeyframe;

float $TStime = `currentTime -q`;

colorEditor;
if (`colorEditor -query -result`) {
   float $rgb[];
   $rgb = `colorEditor -query -rgb`;
   displayRGBColor "timeSliderTickDrawSpecial" $rgb[0] $rgb[1] $rgb[2];
}
selectKey -clear;
selectKey -add -k -t $TStime;
keyframe -tds on;
